{
  "name": "testcheckin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "node -e \"const fs=require('fs');const path=require('path');if(!fs.existsSync('app')){fs.mkdirSync('app',{recursive:true})};if(!fs.existsSync('pages')){fs.mkdirSync('pages',{recursive:true})};const copyDir=(src,dest)=>{if(fs.existsSync(src)){try{fs.readdirSync(src).forEach(file=>{const srcPath=path.join(src,file);const destPath=path.join(dest,file);if(fs.statSync(srcPath).isDirectory()){if(!fs.existsSync(destPath)){fs.mkdirSync(destPath,{recursive:true})};copyDir(srcPath,destPath);}else{fs.copyFileSync(srcPath,destPath);}});}catch(err){console.error('Warning:',err.message);}}};copyDir('src/app','app');copyDir('src/pages','pages');\" && next build"
  },
  "dependencies": {
    "@sky-mavis/tanto-connect": "^0.0.11",
    "@sky-mavis/tanto-wagmi": "^0.0.6",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@types/lodash": "^4.17.16",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "next": "14.0.4",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "wagmi": "^0.12.19",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "10.4.16",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "ignore-loader": "^0.1.2",
    "postcss": "8.4.31",
    "tailwindcss": "3.3.5",
    "typescript": "^5"
  }
}
